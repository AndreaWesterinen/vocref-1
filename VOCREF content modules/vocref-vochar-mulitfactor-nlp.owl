Prefix(owl:=<http://www.w3.org/2002/07/owl#>)
Prefix(rdf:=<http://www.w3.org/1999/02/22-rdf-syntax-ns#>)
Prefix(xml:=<http://www.w3.org/XML/1998/namespace>)
Prefix(xsd:=<http://www.w3.org/2001/XMLSchema#>)
Prefix(rdfs:=<http://www.w3.org/2000/01/rdf-schema#>)


Ontology(<http://vocref.github.io/ontologies/vocref-modules/vocref-vochar-multifactor-nlp.owl>
Import(<http://vocref.github.io/ontologies/vocref-top/1.0-SNAPSHOT>)

Declaration(Class(<http://vocref.github.io/ontologies/vocref-modules/vocref-complex-characteristics-1#NaturalLanguage>))
AnnotationAssertion(rdfs:label <http://vocref.github.io/ontologies/vocref-modules/vocref-complex-characteristics-1#NaturalLanguage> "natural language"@en)
AnnotationAssertion(rdfs:comment <http://vocref.github.io/ontologies/vocref-modules/vocref-complex-characteristics-1#NaturalLanguage> "A language currently or formerly used by people. A language developed from or for use in spoken, written, read, signed, or other communications between people, such as English, Czech, Dinka, Tagalog, or Peruvian Sign Language."@en)
Declaration(Class(<http://vocref.github.io/ontologies/vocref-modules/vocref-complex-characteristics-1#PartOfSpeech>))
AnnotationAssertion(rdfs:label <http://vocref.github.io/ontologies/vocref-modules/vocref-complex-characteristics-1#PartOfSpeech> "part of speech"@en)
AnnotationAssertion(rdfs:comment <http://vocref.github.io/ontologies/vocref-modules/vocref-complex-characteristics-1#PartOfSpeech> "A part of speech is a linguistic category grouping lexical items by their behavior in a language's syntax or morphology. Examples of parts of speech include noun, verb, proper noun, and preposition."@en)
Declaration(Class(<http://vocref.github.io/ontologies/vocref-modules/vocref-complex-characteristics-1#VONaturalLanguageSupport>))
AnnotationAssertion(rdfs:label <http://vocref.github.io/ontologies/vocref-modules/vocref-complex-characteristics-1#VONaturalLanguageSupport> "natural language support"@en)
AnnotationAssertion(rdfs:comment <http://vocref.github.io/ontologies/vocref-modules/vocref-complex-characteristics-1#VONaturalLanguageSupport> "A type of VO Characteristic.  Instances are varyingly complex features, made up of facts about the natural language support included in a specific VO for a specific language. These facts are  bundled as a single, multi-factor feature because accurate description and understanding of the linguistic support offered by the VO in the particular language requires taking the assertions together; they are not independent.  For example, a VO may have lexical support at different levels in different languages, or use different part-of-speech tag sets for different languages. "@en)
SubClassOf(<http://vocref.github.io/ontologies/vocref-modules/vocref-complex-characteristics-1#VONaturalLanguageSupport> <http://vocref.github.io/ontologies/vocref-top#VOCharacteristic>)
Declaration(Class(<http://vocref.github.io/ontologies/vocref-modules/vocref-vochar-multifactor-nlp.owl#PartOfSpeechClassificationSystem>))
AnnotationAssertion(rdfs:label <http://vocref.github.io/ontologies/vocref-modules/vocref-vochar-multifactor-nlp.owl#PartOfSpeechClassificationSystem> "Part of Speech classification system"@en)
AnnotationAssertion(<http://vocref.github.io/ontologies/vocref-top#alternateLabel> <http://vocref.github.io/ontologies/vocref-modules/vocref-vochar-multifactor-nlp.owl#PartOfSpeechClassificationSystem> "part of speeck tag set"@en)
AnnotationAssertion(rdfs:comment <http://vocref.github.io/ontologies/vocref-modules/vocref-vochar-multifactor-nlp.owl#PartOfSpeechClassificationSystem> "A VO representing parts of speech, designed and used for the classification of lexical item occurrances in some text or corpus according to the parts of speech those lexical item occurances. Typically a controlled vocabulary."@en)
SubClassOf(<http://vocref.github.io/ontologies/vocref-modules/vocref-vochar-multifactor-nlp.owl#PartOfSpeechClassificationSystem> <http://vocref.github.io/ontologies/vocref-top#VO>)
Declaration(ObjectProperty(<http://vocref.github.io/ontologies/vocref-modules/vocref-complex-characteristics-1#hasLexiconLanguage>))
AnnotationAssertion(rdfs:comment <http://vocref.github.io/ontologies/vocref-modules/vocref-complex-characteristics-1#hasLexiconLanguage> "This property connects a VO Natural Language Support characteristic with one of the components of that suupport feature, if present: a lexicon in the specified (object) Natural Language."@en)
AnnotationAssertion(rdfs:label <http://vocref.github.io/ontologies/vocref-modules/vocref-complex-characteristics-1#hasLexiconLanguage> "has Lexicon in Language"@en)
ObjectPropertyDomain(<http://vocref.github.io/ontologies/vocref-modules/vocref-complex-characteristics-1#hasLexiconLanguage> <http://vocref.github.io/ontologies/vocref-modules/vocref-complex-characteristics-1#VONaturalLanguageSupport>)
ObjectPropertyRange(<http://vocref.github.io/ontologies/vocref-modules/vocref-complex-characteristics-1#hasLexiconLanguage> <http://vocref.github.io/ontologies/vocref-modules/vocref-complex-characteristics-1#NaturalLanguage>)
Declaration(ObjectProperty(<http://vocref.github.io/ontologies/vocref-modules/vocref-complex-characteristics-1#hasPartOfSpeechClassificationSystem>))
AnnotationAssertion(rdfs:comment <http://vocref.github.io/ontologies/vocref-modules/vocref-complex-characteristics-1#hasPartOfSpeechClassificationSystem> "This property connects a VO Natural Language Support characteristic with one of the components of that suupport feature, if present: a particular part of speech classification system used to tag parts of speech in the lexicon that is part of that NL Support."@en)
AnnotationAssertion(rdfs:label <http://vocref.github.io/ontologies/vocref-modules/vocref-complex-characteristics-1#hasPartOfSpeechClassificationSystem> "has Part of Speech Classification System"@en)
ObjectPropertyDomain(<http://vocref.github.io/ontologies/vocref-modules/vocref-complex-characteristics-1#hasPartOfSpeechClassificationSystem> <http://vocref.github.io/ontologies/vocref-modules/vocref-complex-characteristics-1#VONaturalLanguageSupport>)
ObjectPropertyRange(<http://vocref.github.io/ontologies/vocref-modules/vocref-complex-characteristics-1#hasPartOfSpeechClassificationSystem> <http://vocref.github.io/ontologies/vocref-top#VO>)
)
